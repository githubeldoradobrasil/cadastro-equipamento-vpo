<mvc:View
	controllerName="cadastroequip_fornecedor_vpo.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
    <f:SimpleForm id="SimpleFormTechInfo"
			editable="false"
			layout="ResponsiveGridLayout"
			title="{i18n>TechInfo}"
			labelSpanL="4"
            adjustLabelSpan="false"
			labelSpanM="4"
			emptySpanL="1"
			emptySpanM="1"
			columnsL="1"
			columnsM="1"
            singleContainerFullSize="false">
			<f:content>
				<Label text="{i18n>TpObjTec}"/>
				<Text text="{TpObjTecText}"/>
				<Label text="{i18n>CodTpEquip}"/>
				<Text text="{CodTpEquipText}"/>
				<Label text="{i18n>AlturaVeiculo}"/>
				<Text text="{AlturaVeiculo}"/>
				<Label text="{i18n>LarguraVeiculo}"/>
				<Text text="{LarguraVeiculo}"/>
				<Label text="{i18n>CompriVeiculo}"/>
				<Text text="{CompriVeiculo}"/>
				<Label text="{i18n>UniComprimento}"/>
				<Text text="{UniComprimento}"/>
				<Label text="{i18n>Tara}"/>
				<Text text="{Tara}"/>
				<Label text="{i18n>UniPeso}"/>
				<Text text="{UniPeso}"/>
				<Label text="{i18n>CapMax}"/>
				<Text text="{CapMax}"/>
				<Label text="{i18n>Lotacao}"/>
				<Text text="{Lotacao}"/>
				<Label text="{i18n>VolumCarregamentoCarga}"/>
				<Text text="{VolumCarregamentoCarga}"/>
				<Label text="{i18n>UniVolumeCarga}"/>
				<Text text="{UniVolumeCarga}"/>
				<Label text="{i18n>AlturaEspCarga}"/>
				<Text text="{AlturaEspCarga}"/>
				<Label text="{i18n>UniCompEspacoCarga}"/>
				<Text text="{UniCompEspacoCarga}"/>
				<Label text="{i18n>LarguraMaxCarga}"/>
				<Text text="{LarguraMaxCarga}"/>
				<Label text="{i18n>CompriMaxCarga}"/>
				<Text text="{CompriMaxCarga}"/>
				<Label text="{i18n>NumCompartimentos}"/>
				<Text text="{NumCompartimentos}"/>
				<Label text="{i18n>Rastreador}"/>
				<Text text="{path: 'Rastreador' , formatter: '.formatter.isLicenciado' }"/>
				<Label text="{i18n>TipoRastreador}"/>
				<Text text="{TipoRastreadorText}"/>
			</f:content>
		</f:SimpleForm>
    </VBox>
</mvc:View>